        <fullName>Ending_Unit_Greater_Than_Starting_Unit</fullName>
        <active>true</active>
        <description>EndingUnit should be greater than StartingUnit</description>
        <errorConditionFormula>IF(
   OR(
      ISPICKVAL( ProductRatePlanCharge__r.Model__c , &apos;Overage Pricing&apos;),
      ISPICKVAL( ProductRatePlanCharge__r.Model__c , &apos;Tiered with Overage Pricing&apos;)
   ), 
   FALSE, 
   EndingUnit__c  &lt; StartingUnit__c)</errorConditionFormula>
        <errorDisplayField>EndingUnit__c</errorDisplayField>
        <errorMessage>EndingUnit should be greater than StartingUnit</errorMessage>
    </validationRules>
